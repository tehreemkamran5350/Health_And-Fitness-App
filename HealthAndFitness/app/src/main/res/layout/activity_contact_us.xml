<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_about"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="Contact Us"
        android:paddingTop="10dp"
        android:paddingLeft="110dp"
        android:layout_centerHorizontal="true"
        android:textColor="@color/Black"
        android:background="@color/ThemeColor"
        />
    <ImageView
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/tv_about"
        android:src="@drawable/contact"
        android:id="@+id/img_contact"
        />
    <TextView
        android:layout_below="@+id/img_contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        android:textStyle="italic"
        android:layout_marginTop="10dp"
        android:textColor="@color/Black"
        android:layout_centerHorizontal="true"
        android:text="Give your Feedback at:"
        android:id="@+id/contact_app"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/contact_app"
        android:layout_marginLeft="70dp"
        android:textColor="@color/Black"
        android:textStyle="italic"
        android:textSize="17sp"
        android:id="@+id/tv_Earbab"
        android:text="-bsef15m004@pucit.edu.pk"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_Earbab"
        android:layout_marginLeft="70dp"
        android:textColor="@color/Black"
        android:textStyle="italic"
        android:textSize="17sp"
        android:text="-bsef15m022@pucit.edu.pk"
        android:id="@+id/tv_Emisbah"/>

    <TextView
        android:id="@+id/tv_Etehreem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_Emisbah"
        android:layout_marginLeft="70dp"
        android:textColor="@color/Black"
        android:textStyle="italic"
        android:textSize="17sp"
        android:text="-bsef15m025@pucit.edu.pk"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_Etehreem"
        android:layout_marginLeft="70dp"
        android:textColor="@color/Black"
        android:textStyle="italic"
        android:textSize="17sp"
        android:text="-bsef15m039@pucit.edu.pk"
        android:id="@+id/tv_Easma"/>

</RelativeLayout>
